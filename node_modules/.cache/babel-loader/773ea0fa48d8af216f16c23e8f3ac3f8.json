j-select-user-by-dep>\r\n              </a-form-item>\r\n            </a-col>\r\n            <a-col :xl=\"6\" :lg=\"7\" :md=\"8\" :sm=\"24\">\r\n              <a-form-item label=\"客户\">\r\n                <j-dict-select-tag v-model=\"queryParam.customerId\" dictCode=\"bas_customer,name,id\"/>\r\n              </a-form-item>\r\n            </a-col>\r\n            <a-col :xl=\"4\" :lg=\"6\" :md=\"7\" :sm=\"24\">\r\n              <a-form-item label=\"是否通过\">\r\n                <j-dict-select-tag v-model=\"queryParam.isApproved\" dictCode=\"yn\"/>\r\n              </a-form-item>\r\n            </a-col>\r\n            <a-col :xl=\"4\" :lg=\"6\" :md=\"7\" :sm=\"24\">\r\n              <a-form-item label=\"是否作废\">\r\n                <j-dict-select-tag v-model=\"queryParam.isVoided\" dictCode=\"yn\"/>\r\n              </a-form-item>\r\n            </a-col>\r\n          </template>\r\n\r\n          <span style=\"float: left;overflow: hidden;\" class=\"table-page-search-submitButtons\">\r\n            <a-col :md=\"6\" :sm=\"24\">\r\n              <a-button type=\"primary\" @click=\"searchQuery\" icon=\"search\">查询</a-button>\r\n              <a-button @click=\"searchReset\" icon=\"reload\" style=\"margin-left: 8px\">重置</a-button>\r\n              <j-super-query v-if=\"toggleSearchStatus\" style=\"margin-left: 16px\" :fieldList=\"fieldList\" ref=\"superQueryModal\" @handleSuperQuery=\"handleSuperQuery\"></j-super-query>\r\n              <a @click=\"handleToggleSearch\" style=\"margin-left: 8px\">\r\n                {{ toggleSearchStatus ? '收起' : '展开' }}\r\n                <a-icon :type=\"toggleSearchStatus ? 'up' : 'down'\"/>\r\n              </a>\r\n            </a-col>\r\n          </span>\r\n\r\n        </a-row>\r\n      </a-form>\r\n    </div>\r\n    <!-- 查询区域-END -->\r\n\r\n    <!-- 操作按钮区域 -->\r\n    <div class=\"table-operator\">\r\n      <a-button @click=\"myHandleAdd\" type=\"link\" icon=\"plus\">新增</a-button>\r\n      <!--      <a-button type=\"link\"  icon=\"download\