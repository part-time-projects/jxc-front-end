perator\">\r\n      <a-button type=\"link\" @click=\"myHandleAdd\" icon=\"plus\">新增</a-button>\r\n      <!--      <a-button type=\"link\" icon=\"download\" @click=\"handleExportXls('客户')\">导出</a-button>\r\n      <a-upload name=\"file\" :showUploadList=\"false\" :multiple=\"false\" :headers=\"tokenHeader\" :action=\"importExcelUrl\" @change=\"handleImportExcel\">\r\n        <a-button type=\"link\" icon=\"import\">导入</a-button>\r\n      </a-upload>-->\r\n    </div>\r\n\r\n    <!-- table区域-begin -->\r\n    <div>\r\n      <a-table\r\n        ref=\"table\"\r\n        size=\"middle\"\r\n        bordered\r\n        rowKey=\"id\"\r\n        :scroll=\"{ x: 1600}\"\r\n        class=\"j-table-force-nowrap\"\r\n        :columns=\"columns\"\r\n        :dataSource=\"dataSource\"\r\n        :pagination=\"ipagination\"\r\n        :loading=\"loading\"\r\n        @change=\"handleTableChange\">\r\n\r\n        <a slot=\"code\" @click=\"myHandleDetail(record)\" slot-scope=\"text, record\">{{text}}</a>\r\n\r\n        <span slot=\"action\" slot-scope=\"text, record\">\r\n          <a @click=\"myHandleEdit(record)\">编辑</a>\r\n          <a-divider type=\"vertical\" />\r\n          <a href=\"javascript:;\" @click=\"handleAddSub(record)\">添加物料</a>\r\n          <a-divider type=\"vertical\" />\r\n          <a-dropdown>\r\n            <a class=\"ant-dropdown-link\">更多 <a-icon type=\"down\" /></a>\r\n            <a-menu slot=\"overlay\">\r\n              <!-- <a-menu-item>\r\n                <a href=\"javascript:;\" @click=\"handleAddSub(record)\">添加物料</a>\r\n              </a-menu-item> -->\r\n              <a-menu-item>\r\n                <a-popconfirm title=\"确定删除吗?\" @confirm=\"() => handleDelete(record.id)\">\r\n                  <a>删除</a>\r\n                </a-popconfirm>\r\n              </a-menu-item>\r\n            </a-menu>\r\n          </a-dropdown>\r\n        </span>\r\n\r\n      </a-table>\r\n    </div>\r\n\r\n    <basMaterial-modal ref=\"modalForm\" @ok=\"modalFormOk\"></basMateria